{
    "workbench.colorTheme": "Tokyo Night Storm",
    // Window settings
    "window.menuBarVisibility": "visible",
    "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
    "workbench.editor.enablePreview": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.statusBar.visible": true,
    "workbench.startupEditor": "none",
    // Editor settings
    "editor.autoIndent": "full",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 3,
    "editor.defaultFormatter": null,
    "editor.fontFamily": "Maple Mono NF, MesloLGS NF, FiraCode NF Ret",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.letterSpacing": 0.1,
    "editor.lineHeight": 18,
    "editor.minimap.side": "right",
    "editor.suggestSelection": "first",
    "editor.scrollbar.vertical": "hidden",
    "editor.tabSize": 4,
    // Security settings
    "security.workspace.trust.untrustedFiles": "newWindow",
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.enabled": false,
    "extensions.autoUpdate": true,
    "files.autoGuessEncoding": true,
    // Git settings
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    // Terminal settings
    "terminal.external.linuxExec": "kitty",
    "terminal.explorerKind": "both",
    "terminal.sourceControlRepositoriesKind": "both",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.fontFamily": "Maple Mono NF, MesloLGS NF, FiraCode NF Ret",
    "terminal.integrated.fontSize": 13,
    // Telemetry settings
    "telemetry.telemetryLevel": "off",
    // Python
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.tabSize": 4,
    },
    "python.analysis.autoImportCompletions": true,
    "pytgon.analysis.autoSearchPaths": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.extraPaths": [],
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.autoComplete.extraPaths": [],
    "python.condaPath": "/home/effi/conda/bin/conda",
    "python.diagnostics.sourceMapsEnabled": true,
    "python.languageServer": "Pylance",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "isort.args": [
        "--profile",
        "black"
    ],
    // Jupyter
    "jupyter.askForKernelRestart": false,
    "python.defaultInterpreterPath": "~/venv/jarpy/bin",
    "jupyter.askForRestart": false,
    "jupyter.autoStartKernel": true,
    "jupyter.interactiveWindow.codeLens.enabled": true,
    "jupyter.interactiveWindow.cursorNavigation": true,
    "notebook.lineNumbers": "on",
    "notebook.output.fontFamily": "Fira Code Regular",
    "notebook.output.scrolling": true,
    "notebook.scrollBeyondLastLine": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    // Docker
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "docker.commands.attach": "${containerCommand} exec -it ${containerId} zsh",
    "docker.commands.commit": "docker commit ${containerId} ${image}",
    "docker.commands.inspect": "docker inspect ${containerId}",
    "docker.commands.kill": "docker kill ${containerId}",
    "docker.commands.logs": "docker logs ${containerId}",
    "docker.commands.ps": "docker ps -a",
    "docker.commands.pull": "docker pull ${image}",
    "docker.commands.push": "docker push ${image}",
    "docker.commands.rename": "docker rename ${containerId} ${containerName}",
    "docker.commands.restart": "docker restart ${containerId}",
    "docker.commands.rm": "docker rm ${containerId}",
    "docker.commands.rmi": "docker rmi ${image}",
    // Shell
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "bashIde.explainshellEndpoint": "http://localhost:5000",
    "[dotenv]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "workbench.colorCustomizations": {},
    "terminal.integrated.defaultProfile.linux": "zsh",
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": false,
        "scminput": false
    },
    "cSpell.userWords": [
        "rclpy",
        "REUSEADDR"
    ],
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "zig": "zig run",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "php": "php",
        "python": "$HOME/venv/jarpy/bin/python", //"python -u",
        "perl": "perl",
        "perl6": "perl6",
        "ruby": "ruby",
        "go": "go run",
        "lua": "lua",
        "groovy": "groovy",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "bat": "cmd /c",
        "shellscript": "bash",
        "fsharp": "fsi",
        "csharp": "scriptcs",
        "vbscript": "cscript //Nologo",
        "typescript": "ts-node",
        "coffeescript": "coffee",
        "scala": "scala",
        "swift": "swift",
        "julia": "julia",
        "crystal": "crystal",
        "ocaml": "ocaml",
        "r": "Rscript",
        "applescript": "osascript",
        "clojure": "lein exec",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "racket": "racket",
        "scheme": "csi -script",
        "ahk": "autohotkey",
        "autoit": "autoit3",
        "dart": "dart",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "haskell": "runghc",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "lisp": "sbcl --script",
        "kit": "kitc --run",
        "v": "v run",
        "sass": "sass --style expanded",
        "scss": "scss --style expanded",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "sml": "cd $dir && sml $fileName",
        "mojo": "mojo run"
    },
    "editor.tabCompletion": "on",
    "C_Cpp.autocompleteAddParentheses": true,
    "emmet.useInlineCompletions": true
}